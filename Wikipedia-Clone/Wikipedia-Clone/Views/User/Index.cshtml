@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Wikipedia_Clone.Models.ApplicationUser

<h2>
    Users
</h2>

@foreach (Wikipedia_Clone.Models.ApplicationUser user in ViewBag.Users)
{
    @Html.Partial("_UserShow", user)
    @Html.ActionLink("View user", "Show", "User", new { user.Id }, new { @class = "btn btn-default" })
}

