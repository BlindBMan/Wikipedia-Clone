@model Wikipedia_Clone.Models.Article

<div>
    <h1>
        @Model.Title
    </h1>

    <p>
        @Model.Content
    </p>

    <small>
        @Model.Date.ToString()
    </small>

    <p>
        @Model.Category.CategoryTitle
    </p>

    @Html.ActionLink("View article", "Show", "Article",  new { Model.Id }, null)
    @Html.ActionLink("Edit article", "Edit", "Article",  new { Model.Id }, null)
</div>


