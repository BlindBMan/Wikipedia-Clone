@model Wikipedia_Clone.Models.Article

    <div>
        <h1>
            @Model.Title
        </h1>

        <p>
            @Model.Content
        </p>

        <p>
            @Model.User.UserName
        </p>

        <small>
            @Model.Date.ToString()
        </small>

        <p>
            @Model.Category.CategoryTitle
        </p>
        @if (Model.Protected == false)
        {
            @Html.ActionLink("Edit article", "Edit", "Article", new { Model.Id }, null)
        }
    </div>


